    <header class="intro">
        <div class="intro-body">
            <div class="container">
                <div class="row">
                    <div class="col-md-8 col-md-offset-2">
                        <h1 class="brand-heading">where to</h1>
                       <!--  <p>Tell us about your preferences.<br>Where do you want to go?<br>When do you want to travel?</p> -->

                        <!-- <p class="intro-text">for the spontaneous traveler.<br>satisfy your wanderlust, find your next destination.</p> -->
                          <div>
                          	<%= form_for @detail do |f| %>
	                            <div class="calendar">
	                                <div class='col-md-5'>
	                                    <div class="form-group">
	                                        <div class='input-group date' id='datetimepicker6'>
	                                            <%= f.date_field :start_date, class: 'form-control', placeholder: 'Start Date', type: 'date'  %>
	                                          
	                                            <span class="input-group-addon">
	                                                <span class="glyphicon glyphicon-calendar"></span>
	                                            </span>
	                                        </div>
	                                    </div>
	                                </div>
	                                <div class='col-md-5'>
	                                    <div class="form-group">
	                                        <div class='input-group date' id='datetimepicker7'>

	                                            <%= f.date_field :end_date, class: 'form-control', placeholder: 'Start Date', type: 'date'  %>
	                                            <span class="input-group-addon">
	                                                <span class="glyphicon glyphicon-calendar"></span>
	                                            </span>
	                                        </div>
	                                    </div>
	                                </div>
	                            </div>
             				
                        </div>


                        <div class="input-group budget">
                          <span class="input-group-addon">$</span>
                          <%= f.text_field :high_price, class: 'form-control', placeholder: "What is your budget?" %>
                         
                            <!--   <span class="input-group-addon">.00</span>-->
                        </div>
                        <%= f.submit "Continue", class: 'btn btn-large btn-warning' %>
                        <% end %>
                    </div>
                </div>
            </div>
        </div>
    </header>

